<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fauzi.utsmobileprogramming">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:required="false"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.fauzi.utsmobileprogramming.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fauzi.utsmobileprogramming.MainActivity" />
        <activity
            android:name="com.fauzi.utsmobileprogramming.ListPertandinganActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.fauzi.utsmobileprogramming.NavBarActivity"
            android:label="@string/title_activity_nav_bar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.fauzi.utsmobileprogramming.DetailPertandinganActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>